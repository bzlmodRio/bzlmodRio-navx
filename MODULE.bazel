module(
    name = "bzlmodrio-navx",
    version = "4.0.435",
    compatibility_level = 2023,
)

bazel_dep(name = "platforms", version = "0.0.4")
bazel_dep(name = "rules_java", version = "5.1.0")
bazel_dep(name = "rules_bazelrio", version = "0.0.9")
bazel_dep(name = "rules_roborio_toolchain", version = "2022-1")
bazel_dep(name = "bzlmodrio-allwpilib", version = "2022.3.1")
bazel_dep(name = "bzlmodrio-opencv", version = "4.5.2-1")
bazel_dep(name = "bzlmodrio-ni", version = "2022.4.0")

setup_bzlmodrio_navx_cpp_dependencies = use_extension("//:maven_cpp_deps.bzl", "setup_bzlmodrio_navx_cpp_dependencies")
use_repo(
    setup_bzlmodrio_navx_cpp_dependencies,
    "bazelrio_com_kauailabs_navx_frc_navx-cpp_headers",
    "bazelrio_com_kauailabs_navx_frc_navx-cpp_linuxathena",
    "bazelrio_com_kauailabs_navx_frc_navx-cpp_linuxathenastatic",
    "bazelrio_com_kauailabs_navx_frc_navx-cpp_sources",
    "bazelrio_com_kauailabs_navx_frc_navx-cpp_windowsx86-64",
    "bazelrio_com_kauailabs_navx_frc_navx-cpp_windowsx86-64static",
)

setup_bzlmodrio_navx_java_dependencies = use_extension("//:maven_java_deps.bzl", "setup_bzlmodrio_navx_java_dependencies")
use_repo(
    setup_bzlmodrio_navx_java_dependencies,
    "com_kauailabs_navx_frc_navx_java",
)
